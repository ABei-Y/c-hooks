#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/encode-fe-lint@1.0.9_typescript@5.2.2/node_modules/encode-fe-lint/lib/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/encode-fe-lint@1.0.9_typescript@5.2.2/node_modules/encode-fe-lint/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/encode-fe-lint@1.0.9_typescript@5.2.2/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/encode-fe-lint@1.0.9_typescript@5.2.2/node_modules/encode-fe-lint/lib/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/encode-fe-lint@1.0.9_typescript@5.2.2/node_modules/encode-fe-lint/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/encode-fe-lint@1.0.9_typescript@5.2.2/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
