#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/codesandbox@2.2.3/node_modules/codesandbox/lib/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/codesandbox@2.2.3/node_modules/codesandbox/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/codesandbox@2.2.3/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/codesandbox@2.2.3/node_modules/codesandbox/lib/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/codesandbox@2.2.3/node_modules/codesandbox/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/codesandbox@2.2.3/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/index.js" "$@"
else
  exec node  "$basedir/../../lib/index.js" "$@"
fi
