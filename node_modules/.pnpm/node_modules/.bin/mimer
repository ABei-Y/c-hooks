#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/mimer@1.1.0/node_modules/mimer/bin/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/mimer@1.1.0/node_modules/mimer/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/mimer@1.1.0/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/mimer@1.1.0/node_modules/mimer/bin/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/mimer@1.1.0/node_modules/mimer/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/mimer@1.1.0/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mimer/bin/mimer" "$@"
else
  exec node  "$basedir/../mimer/bin/mimer" "$@"
fi
