#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/dumi@1.1.54_react-dom@18.2.0_react-router@6.18.0_react@18.2.0_typescript@5.2.2/node_modules/dumi/bin/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/dumi@1.1.54_react-dom@18.2.0_react-router@6.18.0_react@18.2.0_typescript@5.2.2/node_modules/dumi/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/dumi@1.1.54_react-dom@18.2.0_react-router@6.18.0_react@18.2.0_typescript@5.2.2/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/dumi@1.1.54_react-dom@18.2.0_react-router@6.18.0_react@18.2.0_typescript@5.2.2/node_modules/dumi/bin/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/dumi@1.1.54_react-dom@18.2.0_react-router@6.18.0_react@18.2.0_typescript@5.2.2/node_modules/dumi/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/dumi@1.1.54_react-dom@18.2.0_react-router@6.18.0_react@18.2.0_typescript@5.2.2/node_modules:/Users/hxadmin/Desktop/my-hooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dumi/bin/dumi.js" "$@"
else
  exec node  "$basedir/../dumi/bin/dumi.js" "$@"
fi
